
server: 
  port: 8706
  undertow:
    direct-buffers: true
    threads:
      io: 4
      worker: 150

spring: 
  application:
    name: bazl-dna-monitor
  profiles:
    active: dev
  cloud:
    # sentinel监控
    sentinel:
      eager: true      transport:
        dashboard: 127.0.0.1:8850
      datasource:
        ds1:
          nacos:
            server-addr: 127.0.0.1:8848
            dataId: sentinel-dna-gateway.json
            groupId: DEFAULT_GROUP
            data-type: json
            rule-type: flow
    nacos:
      server-addr: 127.0.0.1:8848
      config:
        file-extension: yml
        shared-configs:
          - application-${spring.profiles.active}.${spring.cloud.nacos.config.file-extension}
     
